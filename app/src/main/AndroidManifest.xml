<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rav.testingo" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.rav.testingo.SignActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.rav.testingo.FeedActivity"
            android:label="@string/title_activity_feed" >
        </activity>
        <activity
            android:name="com.example.rav.testingo.RegistrationActivity"
            android:label="@string/title_activity_registration" >
            <action android:name="android.intent.action.GET_CONTENT" />
        </activity>
        <activity
            android:name="com.example.rav.testingo.TestActivity"
            android:label="@string/title_activity_test" >
        </activity>
    </application>

</manifest>
